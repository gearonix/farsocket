{
  "name": "gatefetched",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.4",
  "author": "Uzhanin Egor <egor.uzhanin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/gearonix/gatefetched#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gearonix/gatefetched.git"
  },
  "bugs": "https://github.com/gearonix/gatefetched/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/create-gateway.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "bump": "taze -wi"
  },
  "peerDependencies": {
    "@farfetched/core": "^0.11.4",
    "effector": "^23.0.0",
    "socket.io-client": "^4.5.4"
  },
  "peerDependenciesMeta": {
    "socket.io-client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@farfetched/core": "^0.11.4",
    "@types/node": "^20.11.24",
    "bumpp": "^9.3.0",
    "effector": "^23.2.0",
    "eslint": "^8.57.0",
    "eslint-kit": "^10.18.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "socket.io-client": "^4.7.4",
    "taze": "^0.13.3",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm typecheck"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
